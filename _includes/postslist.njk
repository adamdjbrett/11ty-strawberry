{% for post in postslist | reverse %}<div class="col-md-10 mx-auto p-3 p-md-5 border-bottom">
<h3 class="text-big"><strong><a href="{{ post.url }}">{{ post.data.title }}</a></strong></h3>
<p><time datetime="{{ post.date | htmlDateString }}"><code class="text-dark">{{ post.date | readableDate("LLLL yyyy") }}</code></time></p>
<p>{{ post.data.description }}</p>
</div>{% endfor %}
